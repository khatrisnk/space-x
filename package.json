{
  "name": "space-x",
  "version": "1.0.0",
  "description": "front-end application which would help users list and browse all launches by SpaceX program",
  "main": "main.js",
  "scripts": {
    "clean:dist": "rm dist/*.* || true",
    "build:dev-client": "npm run clean:dist && webpack --config=config/webpack.dev-client.js",
    "build:dev-server": "npm run clean:build && NODE_ENV=development webpack --config=config/webpack.dev-server.js",
    "server:dev": "NODE_ENV=development nodemon --watch src/server --watch config --inspect src/server/main",
    "clean:build": "rm build/*.* || true",
    "build:prod-client": "npm run clean:dist && NODE_ENV=production webpack --config=config/webpack.prod-client.js",
    "build:prod-server": "npm run clean:build && NODE_ENV=production webpack --config=config/webpack.prod-server.js",
    "server:prod": "NODE_ENV=production node src/server/main",
    "lint": "eslint ./src --fix",
    "test:watch": "jest --watch --config=jest.config.js",
    "test": "jest --config=jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khatrisnk/space-x.git"
  },
  "keywords": [
    "space-x",
    "javascript",
    "react",
    "ssr"
  ],
  "author": "Siddhartha Khatri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/khatrisnk/space-x/issues"
  },
  "homepage": "https://github.com/khatrisnk/space-x#readme",
  "devDependencies": {},
